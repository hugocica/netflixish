{
    "name": "netflixish",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-scripts": "^3.0.0"
    },
    "scripts": {
        "build-css": "node-sass-chokidar src/assets/sass/app.scss src/assets/css/app.css --output-style compressed",
        "watch-css": "yarn build-css && node-sass-chokidar src/assets/sass/app.scss src/assets/css/app.css --watch --recursive",
        "start-js": "react-scripts start",
        "start": "npm-run-all -p watch-css start-js",
        "build": "npm run build-css && react-scripts build -p",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject"
    },
    "devDependencies": {
        "@material-ui/core": "^3.9.3",
        "axios": "^0.18.0",
        "bootstrap": "^4.3.1",
        "material-ui": "^0.20.2",
        "node-sass-chokidar": "^1.3.4",
        "npm-run-all": "^4.1.5",
        "react-bootstrap": "^1.0.0-beta.8",
        "react-dom": "^16.6.0",
        "react-redux": "^5.0.7",
        "react-router": "^4.3.1",
        "react-router-dom": "^4.3.1",
        "react-swipeable-views": "^0.13.1",
        "redux": "^4.0.1",
        "redux-thunk": "^2.3.0"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}
